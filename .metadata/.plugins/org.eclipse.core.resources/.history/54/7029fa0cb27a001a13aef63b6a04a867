package exerciciobanco;

public class ContaBanco {
	public String numConta;
	protected String tipo;
	private double saldo;
	private boolean status;
	
	
	double getSaldo() {
		return this.saldo;
	}
	
	void setSaldo(double saldo) {
		this.saldo = saldo;
	}
	
	boolean getStatus() {
		return this.status;
	}
	
	void setStatus(boolean status) {
		this.status = status;
	}
	
	void abrirConta() {
		this.status = true;
	}
	
	
	

}
